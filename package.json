{
  "name": "nodejs-restfull",
  "version": "1.0.0",
  "description": "Sample nodejs restfull api, with mongodb!",
  "main": "index.js",
  "scripts": {
    "test": "jest --runInBand --detectOpenHandles",
    "lint:fix": "eslint --fix '**/*.js' --ignore-pattern node_modules/",
    "lint": "eslint '**/*.js' --ignore-pattern node_modules/",
    "build": "./node_modules/.bin/babel -d ./build ./src",
    "prebuild": "rm -rf build/*",
    "dev": "NODE_ENV=development nodemon --exec babel-node src/server.js --ignore __tests__",
    "start": "NODE_ENV=production node ./build/server.js",
    "prestart": "npm run build"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@babel/runtime": "^7.12.1",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.10.9"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.1",
    "@babel/core": "^7.12.3",
    "@babel/node": "^7.12.1",
    "@babel/plugin-transform-runtime": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "eslint": "^7.11.0",
    "faker": "^5.1.0",
    "jest": "^26.6.0",
    "mongodb-memory-server": "^6.9.2",
    "nodemon": "^2.0.6",
    "supertest": "^5.0.0"
  }
}
